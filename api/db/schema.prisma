// https://www.prisma.io/docs/orm/prisma-migrate

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model GigListing {
  id        String   @id @default(cuid())
  title     String
  date      DateTime
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
